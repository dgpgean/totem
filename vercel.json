
{
  "version": 2,
  "cleanUrls": true,
  "framework": null,
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/(index.tsx|App.tsx|types.ts|constants.ts|components/.*|services/.*)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "camera=(self)"
        }
      ]
    }
  ]
}
